
(* Second test of undefined phase with annotations *)

model main(a:int) returns (e:int)
var
	b : int :: [..,4];
	c : int :: [0,1];
	d : int :: [..,4];
let
	label(eqb) min_phase(2) b = buffer (a when [1,4]);    (* Note - if no minphase, b is set to [1,4] *)
	c = current([2,4], 0, buffer b);
	label(eqd) d = buffer (c when [3,4]);
	e = current([3,4], 0, buffer d);
tel
requiring
	range(1, 3, eqb, eqd)
end
