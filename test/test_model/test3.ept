(* Elementary functions (dummy) *)
fun node_000001 (a : int; b : int) returns (c: int; d: int; e: int)
let 
	c = a;
	d = b;
	e = a+b;
tel

fun node_000002 (a : int; b : int) returns (c: int; d: int; e: int; f: int)
let 
	c = a;
	d = b;
	e = a+b;
	f = a*b;
tel

fun node_000003 (a : int) returns (c: int)
let 
	c = 2*a;
tel

fun node_000004 (a : int; b : int) returns (c: int)
let 
	c = a+3*b;
tel

fun node_000005 (a : int) returns (c: int)
let 
	c = 3*a;
tel

fun node_000006 (a : int; b : int) returns (c: int)
let 
	c = 2*a + b;
tel


model main(data_9:int) returns (data_10 : int; data_11B : int)
var
	data_0 : int :: [0,1];   (* C1_0 *)
	data_1 : int :: [0,1];   (* C1_0 *)
	data_2 : int :: [0,1];   (* C1_0 *)
	data_3 : int :: [0,2];   (* C2_0 *)
	data_4 : int :: [0,2];   (* C2_0 *)
	data_5 : int :: [0,2];   (* C2_0 *)
	data_6 : int :: [0,2];   (* C2_0 *)
	data_7 : int :: [2,12];  (* C4_2 *)
	data_8 : int :: [3,12];  (* C4_3 *)
	data_11 : int :: [2,4];  (* C3_2 *)
let
	(data_0, data_1, data_2) = node_000001(
			data_9,
			(0 fby data_0)
		);
	(data_3, data_4, data_5, data_6) = node_000002(
			(0 fby data_4),
			(data_2 when [0,2])
		);
	data_7 = node_000003(
			(0 fby data_5) when [1,6]
		);
	data_10 = node_000004(
			current([0,2], 0, data_3),
			data_1
		);
	data_8 = node_000005(
			delay(1) data_7
		);
	data_11 = node_000006(
			delay(1) current([1,3], 0, delay(2) data_8),
			data_6 when [1,2]
		);
	data_11B = current([2,4], 0, data_11);
tel


