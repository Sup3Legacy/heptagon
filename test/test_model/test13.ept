(* ../../heptc -v -solphconstr sol_test13.txt test13.ept *)

model main(a:int) returns (e:int)
var
	b : int :: [1,2];
	c : int :: [..,6];
	d : int :: [..,2];
let
	b = buffer (a when? 2);
	c = buffer (b when? 3);
	d = buffer current?(3, 0, (buffer c));
	e = buffer current?(2, 0, (buffer d));
tel




