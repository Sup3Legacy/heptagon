
(* Sixth test of unspecified operators - testing chain of latency constraint *)
(* Similar to test15.ept *)

(* ../../heptc -v -noconstrbuffby -genphconstr constr_test18.txt test18.ept *)


open Test15_interf

model main(a:int) returns (e:int)
var
	b : int :: [..,2];
	c : int :: [..,6];
	d : int :: [..,2];
let
	b = buffer ( f1(a when? 2) );
	c = buffer ( f2(b when? 3) );
	d = buffer current?(3, 0, (f3 (buffer c)) );
	e = buffer current?(2, 0, (f4 (buffer d)) );
tel
requiring
	latency_chain 3 (a -> b -> c)
end


