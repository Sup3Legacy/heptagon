node sum(x :int) = (y :int)
let
  y = x + (0 fby y)
tel

node period << n :int | n > 0 >> () returns (c :bool)
var cpt, next_cpt :int;
let
  next_cpt = if (cpt = n) then 1 else (cpt + 1);
  cpt = 1 fby next_cpt;
  c = (cpt = 1);
tel


node main() = ()
var a,b,c,d,e,f,h,i,j :bool;
let

  a = period<<2>>();
  b = period<<1>>();
  c = period<<4>>();
  f = period<<3>>();
  j = period<<8>>();
  d = period<<16>>();
  e = period<<32>>();
  h = period<<64>>();
  i = period<<128>>();
tel
