type pixel = int^3

const pixel_zero :pixel = [0,0,0]

external val node read_int<<infile :string>>() returns (o :pixel)
external val node write_int<<outfile :string; w,h :int>>(i :pixel) returns (finished :bool)
external val node display_int<<window_name :string; w,h :int>>(i :pixel) returns (finished :bool)
